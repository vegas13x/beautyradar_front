    <fragment
        android:id="@+id/signUpFragmentSecond"
        android:name="com.nick_sib.beauty_radar.view.fragments.SignUpSecondFragment"
        android:label="sign_up_fragment_second"
        tools:layout="@layout/fragment_sign_up_second">
        <argument
            android:name="uid"
            android:defaultValue="err"
            app:argType="string" />
        <argument
            android:name="name"
            android:defaultValue="err"
            app:argType="string" />
        <argument
            android:name="second_name"
            android:defaultValue="err"
            app:argType="string" />
        <action
            android:id="@+id/action_signUpFragmentSecond_to_signUpFragment"
            app:destination="@id/signUpFragment" />
        <action
            android:id="@+id/action_signUpFragmentSecond_to_masterClientsFragment"
            app:destination="@id/masterClientsFragment" />
    </fragment>
    <fragment
        android:id="@+id/logoutFragment"
        android:name="com.nick_sib.beauty_radar.view.fragments.LogoutFragment"
        android:label="fragment_logout"
        tools:layout="@layout/fragment_logout">
        <action
            android:id="@+id/action_logoutFragment_to_authFragment"
            app:destination="@id/welcomeFragment"/>
    </fragment>
    <fragment
        android:id="@+id/masterClientsFragment"
        android:name="com.nick_sib.beauty_radar.view.fragments.MasterClientFragment"
        android:label="fragment_master_client"
        tools:layout="@layout/fragment_master_client">
        <action
            android:id="@+id/action_masterClientsFragment_to_logoutFragment"
            app:destination="@id/logoutFragment" />
        <action
            android:id="@+id/action_masterClientsFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.nick_sib.beauty_radar.view.fragments.SignInFragment"
        android:label="SignInFragment"
        tools:layout="@layout/fragment_sign"/>

</navigation>